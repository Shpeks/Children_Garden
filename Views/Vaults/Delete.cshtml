@model Diplom.Models.Vault



<h3>Вы уверены что хотите удалить?</h3>
<div>
    <h4>Свод</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Название
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VaultName)
        </dd>
        <dt class = "col-sm-2">
            Дата начала
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateStart)
        </dd>
        <dt class = "col-sm-2">
            Дата конца
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateEnd)
        </dd>
        <dt class = "col-sm-2">
            Пользователь
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ApplicationUsers.FirstName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> 
        <a asp-action="Index">Вернуться</a>
    </form>
</div>

